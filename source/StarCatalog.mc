using Toybox.Lang as Lang;

class Star extends Lang.Object {

	var alpha;
	var delta;
	var magnitude;	
}

// A memory compact star catalog
class StarCatalog extends Lang.Object {

	static function decodeStar(i) {
		var star = new Star();
		star.magnitude = i % 10;
		i = i / 10;
		star.delta = (i % 10000 - 1800).toFloat()/20;
		star.alpha = (i / 10000).toFloat()/10;
		return star;
	}

	static function getStar(i) {	
		return decodeStar(StarCatalog.stars[i]);
	}
	
